<tr>
  <td><%= institution.name %></td>
  <td><%= institution.acronym %></td>
  <td class="text-nowrap"><%= institution.city.name %></td>
  <td class="text-nowrap"><%= institution.city.state.acronym %></td>
  <td>
    <div class="btn-group">
      <%= link_to edit_admins_institution_path(institution) do %>
        <%= icon('fas', 'edit', class: 'icon ml-2', 'data-toggle' => 'tooltip', title: t('helpers.edit')) %>
      <% end %>

      <%= link_to admins_institution_path(institution), method: :delete,
                  data: {confirm: t('prompt.confirm')} do %>
        <%= icon('fa', 'trash', class: 'icon ml-2', 'data-toggle' => 'tooltip', title: t('helpers.delete')) %>
      <% end %>
    </div>
  </td>
</tr>
